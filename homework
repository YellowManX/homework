function isPerfectNumber(num){
    let a = 0;
    for(let i = 1; i <= num / 2; i++){
        if(num % i === 0){
            a += i;
        }
    }
    return a
}

function isPerfectNumbers(min, max){
    let result = []
    for(let i = min; i <= max; i++){
        if(i === isPerfectNumber(i)){
            result.push(i)
        }
    }
    return result
}
console.log(isPerfectNumbers(1, 500))
